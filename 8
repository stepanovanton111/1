def prime_factorization_simple(n):
    factors = []
    i = 2
    while i <= n:
        while n % i == 0:
            factors.append(i)
            n //= i
        i += 1
    return factors

number = int(input("Введите целое число больше 1: "))
result = prime_factorization_simple(number)
print(f"Разложение числа {number} на простые множители: {result}")
